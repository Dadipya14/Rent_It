<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
 <class name="Producttable" table="producttable">
  <id name="productid" column="productid"/>
  <property name="productname" column="productname"/>
  <property name="producttype" column="producttype"/>
  <property name="colour" column="colour"/>
  <property name="feature" column="feature"/>
  <property name="dimension" column="dimension"/>
  <property name="price" column="price"/>
  <property name="description" column="description"/>
  <property name="tandc" column="termandconditions"/>
 </class>
</hibernate-mapping>
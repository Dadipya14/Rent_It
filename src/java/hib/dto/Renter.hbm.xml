<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hib.dto">
 <class name="Renter" table="renter">
  <id name="email" column="email"/>
  <property name="name" column="username"/>
  <property name="password" column="password"/>
  <property name="address" column="address"/>
  <property name="productname" column="productname"/>
  <property name="contactNum" column="contactnumber"/>
 </class>
</hibernate-mapping>